<style>
.pre pre {
	width: 60%;
	margin: auto;
	background: #fff;
	border-radius: 10px;
	border: 1px solid #ccc;
	padding: 20px;
	text-align: left;
	font-family: '"Consolas", "Bitstream Vera Sans Mono", "Courier New", Courier, monospace !important';
}
.pre h1{
	font-weight: bold;
	font-size: 20px;
}
</style>
<!--页面标识-->
<div class="insure-step-page pt20 pb20 f14 bgf0">
	<div class="layout tac clearfix pt45 pb45 mt45 mb45">
		<p><img width="461" height="297" src="<%=imageHost%>hz/www/page/404.png" alt="<%=message%>" title="<%=message%>"/></p>
		<%if(locals.env==='development'){%>
		
		<div class="pre">
			<h1>开发模式</h1>
			<pre class="">
				<code>
				<%=error.status%><%=message%>
				</code>
			</pre>
		</div>
		
		<%}else{%>
		<p class="mt45 f20">
			<%=error.status%><%=message%>
		</p>
		<%}%>
		
		<p class="mt15">您可以联系<a href="#" class="tdl">在线客服</a>或致电24小时人工座席 <span class="primary-color"><%=tel%></span></p>
		<div class="mt40">
			<a href="<%=mainHost%>" class="hz-button hz-button-primary">访问首页</a>
			<a href="javascript:;" class="hz-button ml15">返回上一步</a>
		</div>
	</div>
	<!--layout end-->
</div>
<!--insure-step-page end-->