<%
var stylesheets = locals.stylesheets||[];
var cssVersion = locals.cssVersion || '';
var env = locals.settings.env;
if(env === 'development' || env === 'test'){
	cssVersion = new Date().getTime();
}else{
	//cssVersion ='';
}
%>

<%stylesheets.forEach(function(item){%><link rel="stylesheet" href="<%=item%>?v=<%=cssVersion%>">
<%});%>
<link rel="shortcut icon" href="<%=locals.mainHost.replace('http:','').replace('https:','')%>favicon.ico" type="image/x-icon" />